<view><view class="container"><view class="profile-info"><image class="avatar" src="{{userInfo.avatar}}" data-event-opts="{{[['tap',[['onUserInfoClick',['$event']]]]]}}" bindtap="__e"></image><view class="info"><text data-event-opts="{{[['tap',[['onUserInfoClick',['$event']]]]]}}" class="name" bindtap="__e">{{userInfo.nickname||'点击登录'}}</text></view><image class="btn" src="/static/static/images/address_right.png" data-event-opts="{{[['tap',[['onUserInfoClick',['$event']]]]]}}" bindtap="__e"></image></view><view class="user-menu"><view class="item"><navigator class="a" url="/pages/ucenter/order/order"><text class="icon order"></text><text class="txt">我的订单</text></navigator></view><view class="item"><navigator class="a" url="/pages/ucenter/collect/collect"><text class="icon address"></text><text class="txt">我的收藏</text></navigator></view><view class="item"><navigator class="a" url="/pages/ucenter/footprint/footprint"><text class="icon security"></text><text class="txt">我的足迹</text></navigator></view><view class="item"><navigator class="a" url="../address/address"><text class="icon address"></text><text class="txt">地址管理</text></navigator></view><view class="item" style="position:relative;"><button class="contactBtn" type="default" open-type="contact"></button><navigator class="a" url="url" data-event-opts="{{[['tap',[['showTip',['$event']]]]]}}" bindtap="__e"><text class="icon kefu"></text><text class="txt">联系客服</text></navigator></view><view class="item"><navigator class="a" url="/pages/ucenter/feedback/feedback"><text class="icon feedback"></text><text class="txt">意见反馈</text></navigator></view></view></view><block wx:if="{{showLoginDialog}}"><view data-event-opts="{{[['tap',[['onCloseLoginDialog',['$event']]]]]}}" class="dialog-login" bindtap="__e"><view data-event-opts="{{[['tap',[['onDialogBody',['$event']]]]]}}" class="dialog-body" catchtap="__e"><view class="title">请选择登录方式</view><view class="content"><button type="primary" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['onWechatLogin',['$event']]]]]}}" bindgetuserinfo="__e">微信登录</button></view></view></view></block></view>