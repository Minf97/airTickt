.fui-filter__icon {
	transition: all .15s linear;
}
.fui-icon__ani {
	-webkit-transform: rotate(180deg);
	        transform: rotate(180deg);
}
.fui-list__cell {
	width: 100%;
}
.fui-select {
	flex: 1;
	height: 80rpx;
	padding: 32rpx;
	box-sizing: border-box;
	position: relative;
}
.fui-select::after {
	content: '';
	position: absolute;
	left: 0;
	top: 0;
	width: 200%;
	height: 200%;
	border: 1px solid #eee;
	-webkit-transform: scale(.5);
	        transform: scale(.5);
	-webkit-transform-origin: 0 0;
	        transform-origin: 0 0;
	pointer-events: none;
}
.fui-flex__center {
	display: flex;
	align-items: center;
	justify-content: center;
}
.fui-flex__between {
	display: flex;
	align-items: center;
	justify-content: space-between;
}

